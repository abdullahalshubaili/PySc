#this scripts converts str to hex 
#cipher+key+iv to be decrypted later on

cipher = 'CIPHER?'.encode('utf-8')
key = 'BASE64 KEY'.encode('utf-8')
iv = 'BASE64 IV'.encode('utf-8')
cipher1 = cipher.hex()
key1 = key.hex()
iv1 = iv.hex()
print( "cipher in hex: \n" + cipher1 + "\nkey in hex: \n" + key1 + '\niv in hex: \n' + iv1 )
